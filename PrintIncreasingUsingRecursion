package recursion;
import java.util.*;

public class PrintIncreasing {

	public static void main(String[] args) {
		// TODO Auto-generated method

		
		Scanner s = new Scanner(System.in);
		int t = s.nextInt();
		
		PrintIncreasing(t);
		
	}
	
	public static void PrintIncreasing(int t) {
		
		if(t==0) {
			return ;
		}
		
		PrintIncreasing(t-1);
		System.out.println(t);
	}

}
